{
    "name": "laravel/craft",
    "description": "Skeleton application installer for the Laravel framework.",
    "license": "MIT",
    "authors": [
        {
            "name": "Taylor Otwell",
            "email": "taylorotwell@gmail.com"
        }
    ],
    "autoload": {
        "classmap": [
            "src/lib/",
            "tests/"
            ]
    },
    "require": {
        "illuminate/support": "4.1.*",
        "illuminate/filesystem": "4.1.*",
        "illuminate/container": "4.1.*",
        "guzzle/guzzle": "3.7.*",
        "symfony/console": "2.4.*"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*",
        "mockery/mockery": "dev-master"
    },
    "config": {
        "bin-dir": "bin",
        "vendor-dir": "src/vendor"
    },
    "minimum-stability": "dev"
}
